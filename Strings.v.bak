module Strings(
	input VGA_CLK,
	input [3:0] Address,
	output [15:0] Data 
);
always @(posedge VGA_CLK)
	begin
	case(Address)
	00:0000000000000000;
	01:0000000000000000;
	02:0110001100000000;
	03:0110001100000000;
	04:0110001100000000;
	05:0110001101111111;
	06:0111111101100110;
	07:0110001100001100;
	08:0110001100011000;
	09:0110001100110000;
	10:0110001101100011;
	11:0110001101111111;
	12:0000000000000000;
	13:0000000000000000;
	14:0000000000000000;
	15:0000000000000000;
	endcase
	end
endmodule 